<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="baseStyle">
        <item name="android:textSize">@dimen/dimen_sp_14</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="labelBaseStyle">
        <item name="android:gravity">center</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
        <item name="android:textSize">@dimen/dimen_sp_14</item>
        <item name="android:fontFamily">@font/myriad_medium</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="labelBaseStyle.Bold" parent="labelBaseStyle">
        <item name="android:textColor">@color/lightGray</item>
        <item name="android:fontFamily">@font/myriad_geo_bold</item>
    </style>

    <style name="labelBaseStyle.Smaller" parent="labelBaseStyle">
        <item name="android:textSize">@dimen/dimen_sp_12</item>
        <item name="android:textColor">@color/lightGray</item>
    </style>

    <style name="labelSmallStyle" parent="labelBaseStyle">
        <item name="android:textSize">@dimen/dimen_sp_12</item>
        <item name="android:textColor">@color/lightGray</item>
        <item name="android:layout_margin">0dp</item>
    </style>

    <style name="TitleLabelTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/dimen_sp_14</item>
        <item name="android:gravity">left</item>
        <item name="android:textColor">@color/titleLabelTextColor</item>
        <item name="android:fontFamily">@font/myriad_geo_bold</item>
    </style>

    <style name="TitleLabelTextStyle.medium" parent="TitleLabelTextStyle">
        <item name="android:fontFamily">@font/myriad_geo_medium</item>
        <item name="android:textColor">@color/appPrimaryColor</item>
    </style>

    <style name="CardTitleLabelTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/dimen_sp_12</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:ellipsize">end</item>
        <item name="android:lines">1</item>
        <item name="android:fontFamily">@font/myriad_geo_bold</item>
    </style>

    <style name="TitleLabelTextStyle.small" parent="TitleLabelTextStyle">
        <item name="android:textSize">@dimen/dimen_sp_10</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="TitleLabelTextStyle.normal" parent="TitleLabelTextStyle">
        <item name="android:textSize">@dimen/dimen_sp_15</item>
        <item name="android:fontFamily">@font/myriad_geo_medium</item>
    </style>

    <style name="TitleLabelTextStyle.large" parent="TitleLabelTextStyle">
        <item name="android:textSize">@dimen/dimen_sp_17</item>
        <item name="android:fontFamily">@font/myriad_geo_medium</item>
    </style>

    <style name="InputFieldTextStyle">
        <item name="android:textSize">@dimen/dimen_sp_14</item>
        <item name="android:textColor">@color/appPrimaryColor</item>
        <item name="android:textColorHint">@color/appPrimaryColor_30</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:fontFamily">@font/myriad_bold</item>
    </style>

    <style name="InputFieldTextStyleDisabled" parent="InputFieldTextStyle">
        <item name="android:textSize">@dimen/dimen_sp_14</item>
        <item name="android:textColor">@color/editTextDisabledTextColor</item>
        <item name="android:fontFamily">@font/myriad_geo_medium</item>
    </style>

    <style name="InputFieldTextStyleDisabled.Big" parent="InputFieldTextStyleDisabled">
        <item name="android:textSize">@dimen/dimen_sp_20</item>
        <item name="android:textColor">#C8C8C8</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:fontFamily">@font/myriad_geo_bold</item>
    </style>

    <style name="InputFieldTextStyleWithHint">
        <item name="android:textSize">@dimen/dimen_sp_12</item>
        <item name="android:textColor">@color/appPrimaryColor</item>
        <item name="android:textColorHint">@color/lowLightGray</item>
        <item name="android:gravity">start|center_vertical</item>
        <item name="android:fontFamily">@font/myriad_medium</item>
    </style>

    <style name="ActionTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/dimen_sp_13</item>
        <item name="android:textColor">@color/appPrimaryColor</item>
        <item name="android:fontFamily">@font/myriad_geo_mtavruli_bold</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:lineSpacingMultiplier">1.2</item>
    </style>

    <style name="ActionTextStyle.medium" parent="ActionTextStyle">
        <item name="android:fontFamily">@font/myriad_geo_mtavruli_medium</item>
        <item name="android:textSize">@dimen/dimen_sp_14</item>
    </style>

    <style name="ButtonTextStyle" parent="ActionTextStyle">
        <item name="android:textSize">@dimen/dimen_sp_15</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="mySpaceItemHeaderLabelStyle" parent="labelBaseStyle">
        <item name="android:textSize">@dimen/dimen_sp_20</item>
        <item name="android:fontFamily">@font/myriad_geo_mtavruli_bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginLeft">@dimen/dimen_p_30</item>
        <item name="android:layout_marginRight">@dimen/dimen_p_10</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>


    <style name="baseInfoSmallLabelStyle" parent="baseStyle">
        <item name="android:textSize">@dimen/dimen_sp_12</item>
        <item name="android:fontFamily">@font/myriad_medium</item>
        <item name="android:textColor">@color/lightGray</item>
