<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SPStatusTextView">
        <attr name="style" />
        <attr name="android:textAppearance" />

        <attr name="status" format="enum">
            <enum name="success" value="0" />
            <enum name="error" value="1" />
            <enum name="pending" value="2" />
            <enum name="info" value="3" />
        </attr>
    </declare-styleable>
</resources>