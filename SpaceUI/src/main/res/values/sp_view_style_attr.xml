<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="backgroundColor" format="color|reference" />
    <attr name="shadowAlpha" format="fraction" />
    <attr name="shadowColor" format="color|reference" />
    <attr name="shadowRadius" format="dimension|reference" />
    <attr name="topLeftCornerRadius" format="dimension|reference" />
    <attr name="topRightCornerRadius" format="dimension|reference" />
    <attr name="bottomRightCornerRadius" format="dimension|reference" />
    <attr name="bottomLeftCornerRadius" format="dimension|reference" />
    <attr name="roundedCorners" format="dimension|reference" />
    <attr name="shadowOffsetX" format="dimension|reference" />
    <attr name="shadowOffsetY" format="dimension|reference" />
    <attr name="chipWidth" format="dimension|reference"/>
    <attr name="chipHeight" format="dimension|reference"/>
    <attr name="iconRadius" format="dimension|reference"/>
    <attr name="isCircle" format="boolean" />
    <attr name="distractiveTextAppearance" format="reference" />
    <attr name="distractiveBackground" format="reference|color" />

    <attr name="directionIcon" format="enum">
        <enum name="none" value="0" />
        <enum name="left" value="1" />
        <enum name="right" value="2" />
    </attr>

    <attr name="btnIconPadding" format="enum">
        <enum name="normal" value="0" />
        <enum name="large" value="1" />
    </attr>

    <declare-styleable name="sp_view_style">
        <attr name="backgroundColor" />
        <attr name="shadowAlpha" />
        <attr name="shadowColor" />
        <attr name="shadowRadius" />
        <attr name="topLeftCornerRadius" />
        <attr name="topRightCornerRadius" />
        <attr name="bottomRightCornerRadius" />
        <attr name="bottomLeftCornerRadius" />
        <attr name="roundedCorners" />
        <attr name="shadowOffsetX" />
        <attr name="shadowOffsetY" />
        <attr name="isCircle" />
    </declare-styleable>

    <declare-styleable name="sp_base_view">
        <attr name="style" format="reference" />
    </declare-styleable>

</resources>